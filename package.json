{
  "name": "faastsaas",
  "private": true,
  "scripts": {
    "build": "npm run build:nextjs-skeleton && npm run build:auth && npm run build:payu-payment && npm run build:payu-payment-ui && npm run build:ui-react",
    "build:nextjs-skeleton": "npm run build -w @faastsaas/nextjs-skeleton",
    "build:auth": "npm run build -w @faastsaas/auth",
    "build:payu-payment": "npm run build -w @faastsaas/payu-payment",
    "build:payu-payment-ui": "npm run build -w @faastsaas/payu-payment-ui",
    "build:ui-react": "npm run build -w @faastsaas/ui-react",
    "dev:nextjs-skeleton": "npm run dev -w @faastsaas/nextjs-skeleton",
    "dev:auth": "npm run dev -w @faastsaas/auth",
    "dev:payu-payment": "npm run dev -w @faastsaas/payu-payment",
    "dev:payu-payment-ui": "npm run dev -w @faastsaas/payu-payment-ui",
    "dev:ui-react": "npm run dev -w @faastsaas/ui-react",
    "lint": "npm run lint:nextjs-skeleton && npm run lint:auth && npm run lint:payu-payment && npm run lint:payu-payment-ui && npm run lint:ui-react",
    "lint:nextjs-skeleton": "npm run lint -w @faastsaas/nextjs-skeleton",
    "lint:auth": "npm run lint -w @faastsaas/auth",
    "lint:payu-payment": "npm run lint -w @faastsaas/payu-payment",
    "lint:payu-payment-ui": "npm run lint -w @faastsaas/payu-payment-ui",
    "lint:ui-react": "npm run lint -w @faastsaas/ui-react",
    "type-check": "npm run type-check:nextjs-skeleton && npm run type-check:auth && npm run type-check:payu-payment && npm run type-check:payu-payment-ui && npm run type-check:ui-react",
    "type-check:nextjs-skeleton": "npm run type-check -w @faastsaas/nextjs-skeleton",
    "type-check:auth": "npm run type-check -w @faastsaas/auth",
    "type-check:payu-payment": "npm run type-check -w @faastsaas/payu-payment",
    "type-check:payu-payment-ui": "npm run type-check -w @faastsaas/payu-payment-ui",
    "type-check:ui-react": "npm run type-check -w @faastsaas/ui-react",
    "storybook:payu-payment-ui": "npm run storybook -w @faastsaas/payu-payment-ui",
    "storybook:ui-react": "npm run storybook -w @faastsaas/ui-react",
    "prettier-check": "prettier --check .",
    "husky-prepare": "husky"
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "packages/payment/*",
    "packages/react/*"
  ],
  "devDependencies": {
    "husky": "^9.1.6",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8"
  },
  "engines": {
    "node": ">=22.11.0"
  }
}
